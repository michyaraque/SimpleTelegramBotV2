{
  "name": "michyaraque/telegram-component-api",
  "description": "A complete Telegram bot API implementation written in PHP, with support for inline bots!",
  "keywords": [
    "Telegram",
    "API",
    "Telegram Bot"
  ],
  "authors": [
    {
      "name": "Michael Araque",
      "email": "michyaraque@gmail.com",
      "homepage": "https://github.com/telegram-component-api/contributors"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "vlucas/phpdotenv": "5.4.x-dev",
    "guzzlehttp/guzzle": "7.4.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~9"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app",
          "Telegram\\": "app/Libraries/Telegram",
          "Helpers\\": "app/Helpers",
          "Libraries\\": "app/Libraries",
          "Enums\\": "app/Enums",
          "Database\\Factories\\": "database/factories/"
      },
      "files": ["app/Helpers/helper.php"]
  },
  "autoload-dev": {
    "psr-4": {
        "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
